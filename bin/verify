#!/bin/bash

main() {
  source "vars/export"

  duplicity verify \
    --verbosity info \
    --s3-european-buckets \
    --s3-use-new-style \
    --s3-use-server-side-encryption \
    --include-filelist "lists/inclusion-list" \
    "$@" \
    "$S3_DUPLICITY_DIR" "$HOME"

  source "vars/unset"
}

main "$@"
