#!/bin/bash

main() {
  source "$duplicity_dir/vars/export"

  if [ $# -lt 1 ]; then echo "Usage $0 <restore-to>"; exit; fi

  duplicity \
    --verbosity info \
    --s3-european-buckets \
    --s3-use-new-style \
    --s3-use-server-side-encryption \
    --force \
    "$S3_DUPLICITY_DIR" "$1"

  source "$duplicity_dir/vars/unset"
}

main "$@"
